# MySQL MCP Server Configuration
# Copy this file to .env and fill in your database credentials

# Default database to connect to
DEFAULT_DATABASE=local

# Security Settings
MAX_QUERY_RESULTS=1000
ALLOW_DATA_MODIFICATION=true
ALLOW_DROP=false
ALLOW_TRUNCATE=false
READ_ONLY_MODE=false

# Local Database Instance (port 3306)
LOCAL_DB_HOST=localhost
LOCAL_DB_PORT=3306
LOCAL_DB_USER=root
LOCAL_DB_PASSWORD=your_password_here
LOCAL_DB_NAME=your_database_name